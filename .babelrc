{
    "presets": ["next/babel"],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd"
        }
      ],
      [
        "inline-import",
        {
          "extensions": [".css"]
        }
      ],
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
}